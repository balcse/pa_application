{
  "style": [
    {
      "selector": "node",
      "style": {
        "background-color": "#666",
        "label": "data(title)"
      }
    },
    {
      "selector": "edge",
      "style": {
        "width": 2,
        "straight": "bezier",
        "line-color": "#0f0",
        "target-arrow-color": "#000",
        "target-arrow-shape": "triangle",
        "arrow-scale": 5
      }
    },
    {
      "selector": ".eh-handle",
      "style": {
        "background-color": "red",
        "width": 12,
        "height": 12,
        "shape": "ellipse",
        "overlay-opacity": 0,
        "border-width": 12,
        "border-opacity": 0
      }
    },
    {
      "selector": ".eh-hover",
      "style": {
        "background-color": "red"
      }
    },
    {
      "selector": ".eh-source",
      "style": {
        "border-width": 2,
        "border-color": "red"
      }
    },
    {
      "selector": ".eh-target",
      "style": {
        "border-width": 2,
        "border-color": "red",
        "target-arrow-shape": "triangle"
      }
    },
    {
      "selector": ".eh-preview",
      "style": {
        "label": "",
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red"
      }
    },
    {
      "selector": ".eh-ghost-edge",
      "style": {
        "label": "",
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red"
      }
    },
    {
      "selector": ".eh-preview, .eh-ghost-edge",
      "style": {
        "background-color": "red",
        "line-color": "red",
        "target-arrow-color": "red",
        "source-arrow-color": "red"
      }
    }
  ],
  "layout": {
    "name": "breadthfirst",
    "avoidOverlap": true,
    "directed": true
  }
}